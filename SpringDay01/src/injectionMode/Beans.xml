<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="person" class="injectionMode.Person">
        <constructor-arg name="name" value="ceh"></constructor-arg>
        <constructor-arg name="age" value="18"></constructor-arg>
        <constructor-arg name="address" value="安徽工信大"></constructor-arg>
    </bean>
    <bean id="person2" class="injectionMode.Person2">
        <property name="name" value="老王"></property>
        <property name="age" value="18"></property>
        <property name="adress" value="隔壁"></property>
    </bean>
</beans>