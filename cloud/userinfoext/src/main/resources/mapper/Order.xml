<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dao.OrderMapper">

    <select id="getByUserId" parameterType="java.lang.Integer" resultType="Order" >
select * from orders where user_id=#{uid}
    </select>
    <insert id="addOrder" parameterType="Order" useGeneratedKeys="true" keyProperty="id">
insert into order(user_id,number,createtime,note) values (#{userId},#{number},#{createtime}.#{note})
    </insert>
</mapper>